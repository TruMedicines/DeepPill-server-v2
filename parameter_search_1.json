{
  "hyperparameters": {
    "type": "object",
    "properties": {
      "startingWeights": {
        "anyOf": [
          {
            "type": "object",
            "properties": {
              "weights": {
                "type": "string",
                "constant": "imagenet"
              },
              "pretrainEpochs": {
                "type": "number",
                "min": 0,
                "max": 100,
                "scaling": "linear"
              }
            }
          },
          {
            "type": "object",
            "properties": {
              "weights": {
                "type": "string",
                "constant": "none"
              }
            }
          }
        ]
      },
      "lossFunction": {
        "type": "object",
        "properties": {
          "positiveWeight": {
            "type": "number",
            "min": 0.7,
            "max": 1.3,
            "scaling": "linear"
          },
          "negativeWeight": {
            "type": "number",
            "min": 0.7,
            "max": 1.3,
            "scaling": "linear"
          },
          "transform": {
            "type": "string",
            "enum": [
              "linear",
              "max",
              "logarithmic"
            ]
          },
          "transformSumMode": {
            "type": "string",
            "enum": [
              "individual",
              "summed"
            ]
          }
        }
      },
      "neuralNetwork": {
        "type": "object",
        "properties": {
          "core": {
            "type": "string",
            "enum": [
              "resnet",
              "inceptionnet",
              "mobilenet",
              "nasnet"
            ]
          },
          "batchSize": {
            "type": "number",
            "min": 2,
            "max": 24,
            "rounding": 1
          },
          "denseLayerMultiplier": {
            "type": "number",
            "min": 0.8,
            "max": 5.0
          },
          "vectorSize": {
            "type": "number",
            "min": 128,
            "max": 4098,
            "scaling": "logarithmic"
          },
          "dropoutRate": {
            "type": "number",
            "min": 0.3,
            "max": 0.8
          },
          "reduceLRFactor": {
            "type": "number",
            "min": 0.1,
            "max": 0.8
          },
          "reduceLRPatience": {
            "type": "number",
            "min": 3,
            "max": 11
          },
          "optimizer": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "optimizerName": {
                    "type": "string",
                    "constant": "adam"
                  },
                  "learningRate": {
                    "type": "number",
                    "min": 1e-5,
                    "max": 1e-3,
                    "scaling": "logarithmic"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "optimizerName": {
                    "type": "string",
                    "constant": "nadam"
                  },
                  "learningRate": {
                    "type": "number",
                    "min": 1e-5,
                    "max": 1e-3,
                    "scaling": "logarithmic"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "optimizerName": {
                    "type": "string",
                    "constant": "sgd"
                  },
                  "learningRate": {
                    "type": "number",
                    "min": 1e-5,
                    "max": 1e-3,
                    "scaling": "logarithmic"
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "optimizerName": {
                    "type": "string",
                    "constant": "rmsprop"
                  },
                  "learningRate": {
                    "type": "number",
                    "min": 1e-5,
                    "max": 1e-3,
                    "scaling": "logarithmic"
                  }
                }
              }
            ]
          }
        }
      },
      "augmentation": {
        "type": "object",
        "properties": {
          "maxRotation": {
            "type": "number",
            "min": 10,
            "max": 90,
            "rounding": 1
          },
          "rotationEasing": {
            "anyOf": [
              {
                "type": "object",
                "properties": {
                  "easing": {
                    "type": "string",
                    "constant": "epoch"
                  },
                  "rotationEasing": {
                    "type": "number",
                    "min": 1,
                    "max": 100,
                    "scaling": "logarithmic",
                    "rounding": 1
                  }
                }
              },
              {
                "type": "object",
                "properties": {
                  "easing": {
                    "type": "string",
                    "constant": "none"
                  }
                }
              }
            ]
          },
          "piecewiseAffine": {
            "type": "number",
            "min": 0,
            "max": 0.05,
            "scaling": "linear"
          },
          "gaussianNoise": {
            "type": "number",
            "min": 0.0,
            "max": 0.5,
            "scaling": "linear"
          }
        }
      }
    }
  },
  "results": {
    "directory": "results",
    "graphs": false
  },
  "ui": {
    "enabled": true
  },
  "function": {
    "type": "python_function",
    "module": "optimization",
    "name": "trainRound1Optimization"
  },
  "search": {
    "method": "atpe",
    "iterations": 1000
  }
}
