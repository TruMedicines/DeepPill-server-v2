{
  "stepsPerEpoch": 1000,
  "validationSteps": 5,
  "maxQueueSize": 25,
  "epochsBeforeAccuracyMeasurement": 1,
  "testingPrintEvery": 250,
  "datasetSizesToTest": [
    100,
    250,
    500,
    750,
    1000,
    1250,
    1500,
    1750,
    2000,
    2250,
    2500
  ],
  "rotationsToTest": [
    1,
    5,
    10,
    15,
    20,
    25,
    30,
    35,
    40,
    45
  ],
  "numGPUs": 1,
  "enableTensorboard": false,
  "imageGenerationWorkers": 2,
   "lossFunction": {"negativeWeight": 1.0,
                    "positiveWeight": 1.0,
                    "transform": "logarithmic",
                    "transformSumMode": "summed",
                    "batchMode": "hard"},
   "neuralNetwork": {"batchSize": 16.0,
                   "augmentationsPerImage": 4,
                   "core": "inceptionnet",
                   "denseLayerMultiplier": 3,
                   "dropoutRate": 0.5,
                   "optimizer": {"$index": 3,
                                 "learningRate": 1e-3,
                                 "pretrainLearningRate": 1e-3,
                                 "optimizerName": "adam"},
                   "vectorSize": 1024,
                   "reduceLRPatience": 5,
                   "reduceLRFactor": 0.75,
                   "denseActivation": "softsign",
                   "finalActivation": "sigmoid",
                   "epochs": 100
   },
  "startingWeights": {
    "weights": "none"
  },
  "augmentation": {
    "gaussianNoise": 0.35,
    "minRotation": 0,
    "maxRotation": 90,
    "piecewiseAffine": 0.02,
    "minScale": 0.9,
    "maxScale": 1.1,
    "minTranslate": -0.1,
    "maxTranslate": 0.1,
    "minGaussianBlur": 0,
    "maxGaussianBlur": 1,
    "minMotionBlur": 5,
    "maxMotionBlur": 11,
    "rotationEasing": {
      "$index": 0,
      "easing": "epoch",
      "minRotationEasing": 0.35,
      "maxRotationEasing": 0.35
    }
  }
}
