              },
              {
                "type": "object",
                "properties": {
                  "optimizerName": {
                    "type": "string",
                    "constant": "rmsprop"
                  },
                  "learningRate": {
                    "type": "number",
                    "min": 1e-4,
                    "max": 5e-3,
                    "scaling": "logarithmic"
                  },
                  "learningRateDecay": {
                    "type": "number",
                    "min": 0.86,
                    "max": 0.995,
                    "scaling": "linear"
                  }
                }
              }
